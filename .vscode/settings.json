    "terminal.integrated.profiles.linux": {
        "bash": {
            "path": "bash",
            "icon": "terminal-bash"
        }
    },
    "terminal.integrated.defaultProfile.linux": "bash",
    "deno.enablePaths": [
        "./src"
    ],
    "deno.config": "./deno.jsonc",
    "[json]": {
        "editor.defaultFormatter": "denoland.vscode-deno"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "denoland.vscode-deno"
    },
    "[typescript]": {
        "editor.defaultFormatter": "denoland.vscode-deno"
    }
}
